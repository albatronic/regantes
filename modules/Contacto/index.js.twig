<script type="text/javascript">
  function initialize() {
    var latlng = new google.maps.LatLng(36.79976, -3.641113);
	
    var myOptions = {
      zoom: 17,
      center: latlng,
      mapTypeId: google.maps.MapTypeId.ROADMAP
    };
    var map = new google.maps.Map(document.getElementById("map_canvas"),
        myOptions);

	 var marker = new google.maps.Marker({
		  position: latlng,
		  map: map,
		  title:"Comunidad de Regantes | Virgen de la Salud"
	  });
  }

</script> {# GOOGLE MAPS #}
