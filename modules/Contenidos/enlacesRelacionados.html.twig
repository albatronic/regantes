<div class="ficherosAdjuntos">

    <div class="contenidoTop"></div>

    <div class="contenidoInter">
            <h6>Enlaces Relacionados</h6>
            <ul class="enlaces_rel">
                {% for enlace in values.enlacesRelacionados.left %}
                    <li>{{enlace.nombre}}<br /><a href="{{enlace.url}}" title="{{enlace.enlace}}" rel="pop-up"><span class="url_enlace">{{enlace.enlace}}</span></a></li>
                {% endfor %}
            </ul>

            <ul class="enlaces_rel right">
                {% for enlace in values.enlacesRelacionados.right %}
                    <li>{{enlace.nombre}}<br /><a href="{{enlace.url}}" title="{{enlace.enlace}}" rel="pop-up"><span class="url_enlace">{{enlace.enlace}}</span></a></li>
                {% endfor %}
            </ul>
    </div>
    <div class="contenidoBottom"></div>
		
</div>