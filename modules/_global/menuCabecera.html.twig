<ul id="nav">
    {% for seccion in values.menuCabecera %}
        <li>
            <a href="{{seccion.url.url}}" 
               title="{{seccion.nombre}}"
               {% if seccion.url.targetBlank %} rel="pop-up"{% endif %}
               {% if values.controller==seccion.controller %}  class="activo"{% endif %}>{{seccion.nombre}}
            </a>
        </li>
    {% endfor %}
</ul>
