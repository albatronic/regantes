<ul id="nav">
    {% for seccion in values.menuCabecera %}
        <li>
            {% if values.controller == seccion.controller %}
                {% set claseCss = "activo" %}
            {% else %}
                {% set claseCss = "" %}
            {% endif %}
            {{ macro.link(seccion.nombre,seccion.url.url,seccion.url.targerBlank,none,claseCss) }}
        </li>
    {% endfor %}
</ul>
